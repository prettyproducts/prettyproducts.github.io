          "_customHTML": "<section class=\"contacts1\" group=\"Contact\" plugins=\"mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showMainText\">\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Card text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Phone\" name=\"showPhone\" checked>\n\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"color\" name=\"cardsBg1\" title=\"BG1\" value=\"#D8EEE1\">\n        <input type=\"color\" name=\"cardsBg2\" title=\"BG2\" value=\"#f3f7f9\">\n        <input type=\"color\" name=\"cardsBg3\" title=\"BG3\" value=\"#FEF2F2\">\n        <input type=\"color\" name=\"cardsBg4\" title=\"BG4\" value=\"#fafafa\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background23.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"mbr-section-head mb-5\">\n            <div class=\"col-12\">\n                <h2 class=\"mbr-section-title mbr-fonts-style align-center mb-2\" mbr-theme-style=\"display-2\" mbr-if=\"showMainTitle\" data-app-selector=\".mbr-section-title\">\n                    <b>Contacts</b>\n                </h2>\n                <h3 class=\"mbr-text mbr-fonts-style align-center mbr-semibold\" mbr-theme-style=\"display-5\" mbr-if=\"showMainText\" data-app-selector=\".mbr-text\">\n                    <b>Want to discuss your feelings? Ready to take things a step further? We'd love to contact with you.</b>\n                </h3>\n            </div>\n        </div>\n        <div class=\"row align-items-center\">\n            <div class=\"col-12 col-md-6\" mbr-class=\"{'col-lg-6': cardsAmount == '2', 'col-lg-3': cardsAmount == '4', 'col-lg-4': cardsAmount == '3'}\">\n                <div class=\"card-wrapper cards-bg1\">\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-title mbr-fonts-style align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-box\">\n                            <b>Italy</b>\n                        </h5>\n                        <p class=\"mbr-section-subtitle mbr-fonts-style mb-3 align-center\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-subtitle, .link\">\n                            Strada Delizia 732, Piano 7, 06508, San Virginia veneto, Belluno\n                        </p>\n                        <p class=\"mbr-section-subtitle mbr-fonts-style mb-3 align-center\" mbr-theme-style=\"display-7\" mbr-if=\"showPhone\" data-app-selector=\".mbr-section-subtitle, .link\">+1-580-5555-000</p>     \n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-6': cardsAmount == '2', 'col-lg-3': cardsAmount == '4', \n                'col-lg-4': cardsAmount == '3'}\">\n                <div class=\"card-wrapper cards-bg2\">\n                    <div class=\"card-box align-left\" mbr-if=\"showTitle || showText || showLink\">\n                        <h5 class=\"card-title mbr-fonts-style align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-box\">\n                            <b>Usa</b>\n                        </h5>\n                        <p class=\"mbr-section-subtitle mbr-fonts-style mb-3 align-center\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-subtitle, .link\">\n                            4433 Stracke Tunnel, Apt. 363, 37537-5519, Hoppechester, Oregon\n                        </p>\n                        <p class=\"mbr-section-subtitle mbr-fonts-style mb-3 align-center\" mbr-theme-style=\"display-7\" mbr-if=\"showPhone\" data-app-selector=\".mbr-section-subtitle, .link\">+1-222-4444-333</p>     \n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6\" mbr-if=\"cardsAmount > 2\" mbr-class=\"{'col-lg-3': cardsAmount == '4', \n                'col-lg-4': cardsAmount == '3'}\">\n                <div class=\"card-wrapper cards-bg3\">\n                    <div class=\"card-box align-left\" mbr-if=\"showTitle || showText || showLink\">\n                        <h5 class=\"card-title mbr-fonts-style align-center\" mbr-theme