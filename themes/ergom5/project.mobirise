          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1\" group=\"Footers\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"8\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <header condition=\"showSocial && showCopyright\">Icon</header>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showSocial\" condition=\"showCopyright\" checked>\n        <select title=\"Align\" name=\"contentAlign\" condition=\"showSocial && showCopyright\">\n            <option value>Left</option>\n            <option value=\"justify-content-center\">Center</option>\n            <option value=\"justify-content-end\" selected>Right</option>\n        </select>\n        <input type=\"range\" name=\"socialCount\" title=\"Count\" min=\"1\" max=\"4\" step=\"1\" value=\"1\" condition=\"showSocial && showCopyright\">\n        <input type=\"color\" title=\"Color\" name=\"socialIconColor\" value=\"#ffffff\" condition=\"showSocial && showCopyright\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#282828\" selected>\n        </fieldset>\n        <input type=\"text\" title=\"Fallback Image\" name=\"videoToImageOnMobile\" condition=\"overlay && bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\" video-to-image-on-mobile=\"{{videoToImageOnMobile}}\">\n        <div class=\"wrapper\">\n            <div class=\"row mbr-white\">\n                <div class=\"col-6 col-md-4 col-lg-2\">\n                    <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-5\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">MORE\n                        FROM ERGO</h5>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">Reviews</li>\n                        <li class=\"mbr-text item-wrap\">Best Products</li>\n                        <li class=\"mbr-text item-wrap\">Versus</li>\n                        <li class=\"mbr-text item-wrap\">Gift Guide</li>\n                    </ul>\n                </div>\n                <div class=\"col-6 col-md-4 col-lg-2\">\n                    <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-5\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                        ABOUT\n                    </h5>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">About ERGO</li>\n                        <li class=\"mbr-text item-wrap\">Newsletter</li>\n                        <li class=\"mbr-text item-wrap\">Sitemap</li>\n                        <li class=\"mbr-text item-wrap\">Careers</li>\n                        <li class=\"mbr-text item-wrap\">Help Center</li>\n                        <li class=\"mbr-text item-wrap\">Licensing</li>\n                    </ul>\n                </div>\n                <div class=\"col-6 col-md-4 col-lg-2\">\n                    <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-5\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                        POLICIES\n                    </h5>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">Privacy Policy</li>\n                        <li class=\"mbr-text item-wrap\">Terms of Use</li>\n                        <li class=\"mbr-text item-wrap\">Cookie Settings</li>\n                        <li class=\"mbr-text item-wrap\">Do Not Sell My Information</li>\n                        <li class=\"mbr-text item-wrap\">Service</li>\n                    </ul>\n                </div>\n                <div class=\"col-6 col-md-4 col-lg-2\">\n                    <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-5\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                        CONTACT\n                    </h5>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">Help Center</li>\n                        <li class=\"mbr-text item-wrap\">Mobirise Forums</li>\n                        <li class=\"mbr-tex