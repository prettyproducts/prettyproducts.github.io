          "_name": "footer01",
          "_sourceTheme": "agentm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer01 agentm5\" group=\"Footer\" global once=\"footers\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"color\" title=\"Secondary Color\" value=\"#009472\" name=\"title\" condition=\"showTitle\" selected>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"List 1\" name=\"showList_1\" checked>\n        <input type=\"checkbox\" title=\"List 2\" name=\"showList_2\" checked>\n        <input type=\"checkbox\" title=\"List Title\" name=\"showListTitle\" condition=\"showList_1 || showList_2\" checked>\n        <input type=\"color\" title=\"Active Color\" value=\"#009472\" name=\"active\" condition=\"showList_1 || showList_2\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#ffffff\" name=\"border\" condition=\"showBorder\" selected>\n        <input type=\"color\" title=\"Container BG\" value=\"#262626\" name=\"container\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#e6e6e6\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-5 card\">\n                <div class=\"title-wrapper\" mbr-if=\"showTitle || showButtons\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-5\"><span>Recruitment </span>Company</h2>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                        <a class=\"btn btn-info-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            Read now\n                        </a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showList_1\">\n                <div class=\"list-wrapper\">\n                    <h3 class=\"list-title mbr-fonts-style\" mbr-if=\"showListTitle\" data-app-selector=\".list-title\" mbr-theme-style=\"display-7\">\n                        MENU\n                    </h3>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap\" data-multiline mbr-article>\n                        <li class=\"item-wrap\">Home</li>\n                        <li class=\"item-wrap\">Clients</li>\n                        <li class=\"item-wrap\">Team</li>\n                        <li class=\"item-wrap\">Contacts</li>\n                        <li class=\"item-wrap\">About us</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-4 card\" mbr-if=\"showList_2\">\n                <div class=\"list-wrapper\">\n                    <h3 class=\"list-title mbr-fonts-style\" mbr-if=\"showListTitle\" data-app-selector=\".list-title\" mbr-theme-style=\"display-7\">\n                        SERVICES\n                    </h3>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap\" data-multiline mbr-article>\n                        <li class=\"item-wrap\">Candidate Sourcing</li>\n                        <li class=\"item-wrap\">Job Posting and Advertising</li>\n                        <li class=\"item-wrap\">Resume Review and Optimization</li>\n                        <li class=\"item-wrap\">Interview Coordination</li>\n                        <li class=\"item-wrap\">Interview Coaching</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 card\">\n                <div class=\"border-wrap\" mbr-if=\"showBorder