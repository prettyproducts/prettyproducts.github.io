          "_name": "testimonials1",
          "_customHTML": "<section class=\"testimonials1\" group=\"Testimonials\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-fullscreen': fullScreen,\n                    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Icon Title\" name=\"showSubTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Album Title\" name=\"showAlbumTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Album Subtitle\" name=\"showAlbumSubTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\">\n        <input type=\"range\" inline=\"\" title=\"Items\" name=\"items\" min=\"0\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"color\" name=\"iconcolor\" value=\"#000000\" title=\"Icons Color\">\n        <input type=\"image\" name=\"cardbg\" title=\"Cover BG\" value=\"@PROJECT_PATH@/assets/images/02.jpg\" parallax=\"\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" parallax=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" condition=\"bg.type == 'color'\">\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#000000\" condition=\"gradientBg &amp;&amp; bg.type == 'color'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n\n            <div class=\"col-md-12 col-lg-6\">\n                <div class=\"card\">\n                    <div class=\"mbr-figure m-auto\">\n                        <img class=\"cover\" src=\"@PROJECT_PATH@/assets/images/02.jpg\" alt=\"Mobirise\">\n                        <h3 class=\"mbr-section-title align-center mbr-bold mbr-white mbr-fonts-style\" mbr-if=\"showAlbumTitle\" mbr-theme-style=\"display-5\">Everything</h3>\n\n                        <h4 class=\"mbr-section-title align-center mbr-bold mbr-white mbr-fonts-style\" mbr-if=\"showAlbumSubTitle\" mbr-theme-style=\"display-7\">2025</h4>\n                    </div>\n\n                </div>\n\n            </div>\n\n\n\n            <div class=\"col-md-12 col-lg-6 py-5 m-auto\">\n                <div class=\"media-content\">\n                    <h1 class=\"mbr-section-title align-left mbr-bold mbr-black pb-3 mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\">\"The most successful DJ and musicwriter of his generation\"</h1>\n\n\n                    <p class=\"mbr-text align-left mbr-bold pb-2 mbr-fonts-style\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\" mbr-theme-style=\"display-7\">DJ MAGAZINE</p>\n\n                    <div class=\"mbr-section-btn pb-3 align-left\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons=\"\" mbr-theme-style=\"display-4\"><a class=\"btn btn-md btn-black-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-shopping-cart mbr-iconfont mbr-iconfont-btn\"></span>Buy\n                            Album</a></div>\n\n\n\n                </div>\n\n\n            </div>\n\n            <div class=\"col-md-12 col-lg-12\">\n                <h3 class=\"mbr-section-subtitle pt-5 mbr-bold align-left mbr-black mbr-light  mbr-fonts-style\" mbr-if=\"showSubTitle\" mbr-theme-style=\"display-5\">Listen on</h3>\n\n                <div class=\"icons-wrap pt-3\" mbr-if=\"items > 0\">\n                    <div class=\"icon-wrap\" mbr-if=\"items > 0\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon-apple socicon\"></span>\n                        <h2 class=\"icon-text mbr-bold align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".icon-text\">ITUNES</h2>\n                    </div>\n\n                    <div class=\"icon-wrap\" mbr-if=\"items > 1\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon-spotify socicon\"></span>\n                        <h2 class=\"icon-text mbr-bold align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".icon-text\">SPOTIFY</h2>\n                    </div>\n\n                    <div class=\"icon-wrap\" mbr-if=\"items > 2\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon-amazon socicon\"></span>\n                        <h2 class=\"icon-text mbr-bold align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".icon-text\">AMAZON</h2>\n                    </div>\n\n\n                </div>\n\n            </div>\n        </div>\n    </div>\n\n</section>",
