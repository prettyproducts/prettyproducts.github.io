"#74a3c7\">\n\t\t<input name=\"activeTabColor\" title=\"Active Tab\" type=\"color\" value=\"#55595c\">\n\t\t<input name=\"activeTabBorderColor\" title=\"Active Border\" type=\"color\" value=\"#5aac98\">\n\t\t<header>Background</header>\n\t\t<fieldset name=\"bg\" parallax=\"\" type=\"background\">\n\t\t\t<input title=\"Image\" type=\"image\" value=\"../_images/background8.jpg\">\n\t\t\t<input selected=\"\" title=\"Color\" type=\"color\" value=\"#ffffff\">\n\t\t\t<input title=\"Video\" type=\"video\" value=\"https://vimeo.com/428046504\">\n\t\t</fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"../_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n\t\t<input checked=\"\" condition=\"bg.type !== 'color'\" name=\"overlay\" title=\"Overlay\" type=\"checkbox\">\n\t\t<input condition=\"overlay&amp;&amp;bg.type !== 'color'\" name=\"overlayColor\" title=\"Overlay Color\" type=\"color\" value=\"#ffffff\">\n\t\t<input condition=\"overlay&amp;&amp;bg.type !== 'color'\" inline=\"\" max=\"1\" min=\"0\" name=\"overlayOpacity\" step=\"0.1\" title=\"Opacity\" type=\"range\" value=\"0.7\">\n\t</mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay&amp;&amp;bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n\t<div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n\t\t<h3 class=\"mbr-section-title mbr-fonts-style align-center\" data-app-selector=\".mbr-section-title\" mbr-if=\"showMainTitle\" mbr-theme-style=\"display-2\">\n\t\t\tFeatures with tabs </h3>\n\t\t<h4 class=\"mbr-section-subtitle mbr-fonts-style align-center\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showMainSubtitle\" mbr-theme-style=\"display-5\">\n\t\t\tSubtitle </h4>\n\t\t<div class=\"row mt-4\">\n\t\t\