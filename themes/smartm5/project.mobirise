sets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"item features-without-image col-12 col-lg-4 item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"number-wrap\" mbr-if=\"showNumber\">\n                            <p class=\"card-number_1 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number_1, .number-wrap\">\n                                <b>1</b>\n                            </p>\n                        </div>\n                        <a href=\"#\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                <b>mobi@rise.com</b>\n                            </h4>\n                        </a>\n                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".card-text\">\n                            Drop us an email, and one of our Current team members will promptly reach out to you.\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-lg-4 item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"number-wrap\" mbr-if=\"showNumber\">\n                            <p class=\"card-number_2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number_2, .number-wrap\">\n                                <b>2</b>\n                            </p>\n                        </div>\n                        <a href=\"#\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                <b>+0-876-543-32-11</b>\n                            </h4>\n                        </a>\n                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".card-text\">\n                            Our team is here to take your calls seven days a week, from 8:00 AM to 8:00 PM.\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-lg-4 item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"number-wrap\" mbr-if=\"showNumber\">\n                            <p class=\"card-number_3 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number_3, .number-wrap\">\n                                <b>3</b>\n                            </p>\n                        </div>\n                        <a href=\"#\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                 