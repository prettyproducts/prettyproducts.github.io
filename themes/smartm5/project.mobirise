          "_sourceTheme": "smartm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"list01 smartm5\" group=\"List\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Number\" name=\"showNumber\" checked=\"\">\n        <input type=\"color\" title=\"Icon BG 1\" value=\"#fff2fb\" name=\"bg_1\" condition=\"showNumber\" selected=\"\">\n        <input type=\"color\" title=\"Icon BG 2\" value=\"#f2fafd\" name=\"bg_2\" condition=\"showNumber\" selected=\"\">\n        <input type=\"color\" title=\"Icon BG 3\" value=\"#fefced\" name=\"bg_3\" condition=\"showNumber\" selected=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"image-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/image2.jpg\" alt=\"Mobirise\">\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"content-wrapper\">\n                    <div class=\"title-wrapper\">\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                            <b>The features of app</b>\n                        </h2>\n                        <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\">\n                            A powerful tool designed to help users\n                        </h3>\n                    </div>\n                    <div class=\"items-wrapper\">\n                        <div class=\"item features-without-image item-mb\">\n                            <div class=\"item-wrapper\">\n                                <div class=\"card-box\">\n                                    <p class=\"card-number_1 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showNumber\" data-app-selector=\".card-number_1\">\n                                        <b>1</b>\n                                    </p>\n                                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                        Personalized workout plans: The app should offer the ability to create\n                                        customized workout plans tailored to the user's fitness goals, experience level,\n                                        and available equipment.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"item features-without-image item-mb\">\n                            <div class=\"item-wrapper\">\n                                <div class=\"card-box\">\n                                    <p class=\"card-number_2 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showNumber\" data-app-selector=\".card-number_2\">\n                                        <b>2</b>\n                                    </p>\n                                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                        Exercise library: A comprehensive library of exercises with detailed\n                                        instructions, images, and videos to ensure users perform exercises with proper\n                                        form and technique.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"item features-without-image item-mb\">\n                            <div class=\"item-wrapper\">\n                                <div class=\"card-box\">\n                                    <p class=\"card-number_3 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showNumber\" data-app-selector=\".card-number_3\">\n                                        <b>3</b>\n                                    </p>\n                                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                        Goal setting: Users should be able to set specific fitness goals, such as weight\n                                        loss, muscle gain, or improved endurance, and the app should help track\n                                        their progress toward these objectives.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                            <a class=\"btn btn-secondary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                <span class=\"socicon socicon-apple mbr-iconfont mbr-iconfont-btn\"></span>\n                                App store\n                            </a>\n                            <a class=\"btn btn-secondary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                <span class=\"socicon socicon-play mbr-iconfont mbr-iconfont-btn\"></span>\n                                Play store\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n  