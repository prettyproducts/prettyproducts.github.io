{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-sm\" mbr-if=\"showContact\">\n                <p class=\"mbr-fonts-style phone-number\" data-app-selector=\".phone-number\" mbr-theme-style=\"display-4\">\n                    +123 456 7890(WhatsApp, Phone)</p>\n            </div>\n            <div class=\"col-12 col-sm\" mbr-if=\"showCopyright\">\n                <p class=\"mbr-text mb-0 mbr-fonts-style mbr-white align-center\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">\n                     Copyright 2025 Mobirise. All Rights Reserved.\n                </p>\n            </div>\n            <div class=\"col-12 col-sm\" mbr-if=\"showSocial\">\n                <div mbr-list=\"\" mbr-list-grow=\"socialItems\" class=\"social-list pb-2 d-flex\" mbr-if=\"socialItems > 0\">\n                    <div class=\"soc-item\">\n                        <a href=\"https://instagram.com/mobirise\" target=\"_blank\">\n                            <span mbr-icon=\"\" class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\">\n                        <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                            <span mbr-icon=\"\" class=\"socicon-facebook socicon mbr-iconfont mbr-iconfont-social\"></span>\n                        </a>\n                    </div>\n\n                    <div class=\"soc-item\">\n                        <a href=\"\" target=\"_blank\">\n                            <span mbr-icon=\"\" class=\"socicon socicon-linkedin\" title=\"linkedin\"></span>\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
