TH@/assets/images/background16.jpg\" selected=\"\">\n            <input type=\"color\" title=\"Color\" value=\"#181818\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row d-flex align-items-center justify-content-center {{contentAlign}}\">\n            <div class=\"col-12 col-xxl-4\">\n                <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">Study course</h1>\n            </div>\n            <div class=\"col-12 col-xxl-8\">\n                <div class=\"col-12\">\n                    <h4 class=\"main-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".main-subtitle\" mbr-if=\"showSubtitle\">Lorem ipsum dolor sit amet, consectetur adipiscing elit</h4>\n                </div>\n                <div class=\"col-12\">\n                    <div class=\"mbr-section-btn\" mbr-buttons=\"\" mbr-theme-style=\"display-4\" mbr-if=\"showButtons\" data-app-selector=\".mbr-section-btn\">\n                        <a class=\"btn btn-primary\" data-app-placeholder=\"Type text\">Guarantee off</a>\n                        <a class=\"btn btn-white-outline\" data-app-placeholder=\"Type text\">Download<span class=\"mobi-mbri mobi-mbri-down\" title=\"down\"></span></a>\n                    </div>\n                </div>\n            </div>\n\n\n        </div>\n        <div class=\"info {{contentAlign}}\">\n            <div class=\"row align-items-center \">\n                <div class=\"col-md-4 col-sm-6 add-info\" mbr-if=\"showFeatures\">\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-0 mb-md-3\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showFeatureSubtitle\">Next intake</h5>\n                    <p class=\"mbr-text mbr-fonts-style add-info-text\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">June 5, 2023</p>\n                </div>\n                <div class=\"col-md-4 col-sm-6 add-info\" mbr-if=\"showFeatures\">\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-0 mb-md-3\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showFeatureSubtitle\">Duration</h5>\n                    <p class=\"mbr-text mbr-fonts-style add-info-text\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">3 months</p>\n                </div>\n                <div class=\"col-md-4 col-sm-6 add-info\" mbr-if=\"showFeatures\">\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-0 mb-md-3\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showFeatureSubtitle\">Speakers</h5>\n                    <p class=\"mbr-text mbr-fonts-style add-info-text\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">professors</p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
