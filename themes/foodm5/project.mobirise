          "_name": "pricing01",
          "_sourceTheme": "foodm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing01 foodm5\" group=\"Pricing\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <input type=\"color\" title=\"Active BG\" value=\"#18C28A\" name=\"activeBg\">\n        <input type=\"color\" title=\"Active Color\" value=\"#ffffff\" name=\"active\">\n        <input type=\"color\" title=\"Border Color\" value=\"#707070\" name=\"border\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" parallax=\"\">\n            <input type=\"color\" title=\"Color\" value=\"#dedede\" selected=\"\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#dedede\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-4 item features-without-image item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"content-wrap\">\n                            <h3 class=\"mbr-price mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-price\">\n                                <b>$6</b>\n                            </h3>\n                            <h4 class=\"mbr-card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-card-title, .\" mbr-if=\"showCardTitle\">\n                                <b>Choco-Nut Parfait</b>\n                            </h4>\n                            <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                                Satisfy your sweet cravings with our Choco-Nut Delight Parfait, a delightful fusion of\n                                chocolate and nuts.\n                            </p>\n                        </div>\n                        <div class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons=\"\" mbr-theme-style=\"display-4\">\n                            <a href=\"\" class=\"btn item-btn btn-primary-outline\" data-app-placeholder=\"Type Text\">\n                                ORDER NOW\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-4 item features-without-image item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"content-wrap\">\n                            <h3 class=\"mbr-price mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-price\">\n                                <b>$8</b>\n                            </h3>\n                            <h4 class=\"mbr-card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-card-title, .\" mbr-if=\"showCardTitle\">\n                                <b>Chocolate Mousse Cake</b>\n                            </h4>\n                            <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">\n                                This masterpiece of a dessert boasts layers of moist chocolate sponge cake, separated by\n                                a velvety smooth chocolate mousse filling.\n                            </p>\n                        </div>\n                        <div class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons=\"\" mbr-theme-style=\"display-4\">\n                            <a href=\"\" class=\"btn item-btn btn-primary-outline\" data-app-placeholder=\"Type Text\">\n                                ORDER NOW\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-4 item features-without-image item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <div class=\"content-wrap\">\n        