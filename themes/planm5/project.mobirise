          "_name": "list01",
          "_sourceTheme": "planm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"list01 planm5\" group=\"List\" plugins=\"mbr-switch-arrow, mobirise-icons\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\" checked=\"\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcon\" checked=\"\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#192227\" name=\"icon\" selected=\"\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Icon BG\" value=\"#F9A600\" name=\"iconBg\" selected=\"\" condition=\"showIcon\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"color\" title=\"Container BG\" value=\"#192227\" name=\"container\" selected=\"\">\n        <header>Cards</header>\n        <select title=\"Amount\" name=\"cardAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n        </select>\n        <input type=\"color\" title=\"Icon Color\" value=\"#fdfcfc\" name=\"cardIcon\" selected=\"\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"color\" title=\"Color 1\" value=\"#F9A600\" name=\"color_1\" selected=\"\" condition=\"showCardTitle\">\n        <input type=\"color\" title=\"Color 2\" value=\"#ADE988\" name=\"color_2\" selected=\"\" condition=\"showCardTitle\">\n        <input type=\"color\" title=\"Color 3\" value=\"#2C8AC0\" name=\"color_3\" selected=\"\" condition=\"showCardTitle\">\n        <input type=\"color\" title=\"Color 4\" value=\"#F06246\" name=\"color_4\" selected=\"\" condition=\"showCardTitle\">\n        <input type=\"color\" title=\"Color 5\" value=\"#B08CE1\" name=\"color_5\" selected=\"\" condition=\"showCardTitle\">\n        <input type=\"checkbox\" title=\"Card Description\" name=\"showDesc\" checked=\"\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"color\" title=\"Border Card\" value=\"#4a5357\" name=\"border\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" selected=\"\" parallax=\"\">\n            <input type=\"color\" title=\"Color\" value=\"#fdfcfc\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-4 item\">\n                <div class=\"content-wrapper\">\n                    <div class=\"title-wrapper\">\n                        <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-setting mobi-mbri\"></span>\n                        </div>\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .icon-wrapper\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                            The finance function\n                        </h2>\n                        <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                            The finance function within an organization\n                        </p>\n                    </div>\n                    <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                        <div class=\"card\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n                                    <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                        Performance\n                                    </h4>\n                                    <span class=\"sign mbr-iconfont mobi-mbri-down mobi-mbri\"></span>\n                                </a>\n                            </div>\n                            <div id=\"collapse1\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                <div class=\"panel-body\">\n                                    <h5 class=\"mbr-fonts-style panel-title\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title\" mbr-if=\"showCardTitle\">\n                                        <span>Financial</span> Planning and Analysis\n                                    </h5>\n                                    <div class=\"desc-wrapper\" mbr-if=\"showDesc\">\n                                        <div class=\"desc-wrap\">\n                                            <p class=\"mbr-fonts-style panel-desc\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-desc, .desc-wrapper\">\n                                                STRATEGIC PLANNING\n                                            </p>\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-more-horizontal mobi-mbri\"></span>\n                                        </div>\n                                    </div>\n                                    <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\" mbr-if=\"showCardText\">\n                                        This area focuses on creating budgets, forecasts, and financial models to project\n                                        the company's financial performance. FP&amp;A professionals analyze historical data,\n                                        market trends, and business strategies to provide insights that guide\n                                        decision-making and strategic planning.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"card\" mbr-if=\"cardAmount > 1\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse2\" aria-expanded=\"false\" aria-controls=\"collapse2\">\n                                    <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                        Flows\n                                    </h4>\n                                    <span class=\"sign mbr-iconfont mobi-mbri-down mobi-mbri\"></span>\n                                </a>\n                            </div>\n                            <div id=\"collapse2\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                <div class=\"panel-body\">\n                                    <h5 class=\"mbr-fonts-style panel-title\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title\" mbr-if=\"showCardTitle\">\n                                        Treasury <span>Management</span>\n                                    </h5>\n                                    <div class=\"desc-wrapper\" mbr-if=\"showDesc\">\n                                        <div class=\"desc-wrap\">\n                                            <p class=\"mbr-fonts-style panel-desc\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-desc, .desc-wrapper\">\n                                                ORGANIZATION\n                                            </p>\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-more-horizontal mobi-mbri\"></span>\n                                        </div>\n                                    </div>\n                                    <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\" mbr-if=\"showCardText\">\n                                        Treasury management involves managing the company's cash flows, liquidity, and\n                                        financial assets. Treasury professionals are responsible for optimizing cash\n                                        management, overseeing banking relationships, and ensuring that the organization has\n                                        adequate funds to meet its operational and strategic needs.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"card\" mbr-if=\"cardAmount > 2\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse3\" aria-expanded=\"false\" aria-controls=\"collapse3\">\n                                    <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                        Investments\n                                    </h4>\n                                    <span class=\"sign mbr-iconfont mobi-mbri-down mobi-mbri\"></span>\n                                </a>\n                            </div>\n                            <div id=\"collapse3\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                <div class=\"panel-body\">\n                                    <h5 class=\"mbr-fonts-style panel-title\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title\" mbr-if=\"showCardTitle\">\n                                        Capital <span>Budgeting and Investment</span> Decisions\n                                    </h5>\n                                    <div class=\"desc-wrapper\" mbr-if=\"showDesc\">\n                                        <div class=\"desc-wrap\">\n                                            <p class=\"mbr-fonts-style panel-desc\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-desc, .desc-wrapper\">\n                                                CASH MANAGEMENT\n                                            </p>\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-more-horizontal mobi-mbri\"></span>\n                                        </div>\n                                    </div>\n                                    <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\" mbr-if=\"showCardText\">\n                                        Finance professionals evaluate potential investments and allocate capital to\n                                        projects or initiatives that are expected to generate returns and add value to the\n                                        company. This involves analyzing the costs, benefits, risks, and potential outcomes\n                                        of various investment opportunities.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"card\" mbr-if=\"cardAmount > 3\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse4\" aria-expanded=\"false\" aria-controls=\"collapse4\">\n                                    <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                        Protect\n                                    </h4>\n                                    <span class=\"sign mbr-iconfont mobi-mbri-down mobi-mbri\"></span>\n                                </a>\n                            </div>\n                            <div id=\"collapse4\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                <div class=\"panel-body\">\n                                    <h5 class=\"mbr-fonts-style panel-title\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title\" mbr-if=\"showCardTitle\">\n                                        <span>Risk</span> Management\n                                    </h5>\n                                    <div class=\"desc-wrapper\" mbr-if=\"showDesc\">\n                                        <div class=\"desc-wrap\">\n                                            <p class=\"mbr-fonts-style panel-desc\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-desc, .desc-wrapper\">\n                                                INSURANCE\n                                            </p>\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-more-horizontal mobi-mbri\"></span>\n                                        </div>\n                                    </div>\n                                    <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\" mbr-if=\"showCardText\">\n                                        Managing financial risks is crucial to protect the company's assets and interests.\n                                        This area involves identifying, assessing, and mitigating various types of risks,\n                                        such as market risk, credit risk, operational risk, and more. Techniques like\n                                        hedging, insurance, and diversification are used to manage risk exposure.\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"card\" mbr-if=\"cardAmount > 4\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingFive\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse5\" aria-expanded=\"false\" aria-controls=\"collapse5\">\n                                    <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                        Reporting\n                                    </h4>\n                                    <span class=\"sign mbr-iconfont mobi-mbri-down mobi-mbri\"></span>\n                                </a>\n                            </div>\n                            <div id=\"collapse5\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFive\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                <div class=\"panel-body\">\n                                    <h5 class=\"mbr-fonts-style panel-title\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title\" mbr-if=\"showCardTitle\">\n                                        Financial <span>Reporting and Accounting</span>\n                                    </h5>\n                                    <div class=\"desc-wrapper\" mbr-if=\"showDesc\">\n                                        <div class=\"desc-wrap\">\n                                            <p class=\"mbr-fonts-style panel-desc\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-desc, .desc-wrapper\">\n                                                ACCOUNTABILITY\n                                            </p>\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-more-horizontal mobi-mbri\"></span>\n                                        </div>\n                                    </div>\n                                    <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\" mbr-if=\"showCardText\">\n                                        The finance function is responsible for accurate and timely financial reporting,\n                                        including preparing financial statements (such as the balance sheet, income\n                                        statement, and cash flow statement) in compliance with relevant accounting standards\n                                        (e.g., GAAP or IFRS).\n                                    </p>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6 item\">\n                <div class=\"image-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/background4.jpg\" alt=\"Mobirise Website Builder\">\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n",
