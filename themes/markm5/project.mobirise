            },
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 100px",
                "@media (max-width: 1440px)": {
                  "padding": "0 45px"
                },
                "@media (max-width: 992px)": {
                  "padding": "0 30px"
                }
              }
            },
            "@media (max-width: 992px)": {
              ".container": {
                "padding": "0 30px"
              }
            },
            ".item:focus, span:focus": {
              "outline": "none"
            },
            ".embla__slide": {
              "display": "flex",
              "justify-content": "center",
              "position": "relative",
              "min-width": "100%",
              "max-width": "100%",
              "padding": "3rem 0",
              "@media (max-width: 767px)": {
                "min-width": "100%",
                "max-width": "100%"
              },
              ".slide-content": {
                "width": "100%",
                "display": "flex",
                "align-items": "center",
                "justify-content": "space-between",
                "@media (max-width: 992px)": {
                  "display": "block"
                },
                ".social-wrapper": {
                  "width": "250px",
                  "position": "relative",
                  "z-index": "1",
                  ".social-row": {
                    "@media (max-width: 992px)": {
                      "display": "flex",
                      "justify-content": "center",
                      "width": "100%",
                      "margin-bottom": "45px"
                    },
                    ".soc-item": {
                      "margin-bottom": "30px",
                      "@media (max-width: 992px)": {
                        "margin": "0 10px"
                      },
                      "a": {
                        "display": "block",
                        "&:hover, &:focus": {
                          ".mbr-iconfont": {
                            "opacity": ".6"
                          }
                        },
                        ".mbr-iconfont": {
                          "color": "@iconSoc",
                          "transition": "all 0.3s ease-in-out"
                        }
                      }
                    }
                  }
                },
                ".item-wrapper": {
                  "position": "relative",
                  "z-index": "1",
                  "margin": "0 auto",
                  "a": {
                    "display": "block",
                    "&:hover, &:focus": {
                      ".item-img .icon-wrapper": {
                        "opacity": "1",
                        "&::before": {
                          "opacity": ".6"
                        }
                      }
                    },
                    ".item-img": {
                      "position": "relative",
                      "display": "flex",
                      "justify-content": "center",
                      "align-items": "center",
                      "img": {
                        "height": "650px",
                        "max-width": "450px",
                        "object-fit": "cover",
                        "transform": "rotate(-14deg)",
                        "@media (max-width: 1400px)": {
                          "max-width": "350px"
                        },
                        "@media (max-width: 1199px)": {
                          "height": "450px",
                          "max-width": "310px"
                        },
                        "@media (max-width: 768px)": {
                          "transform": "rotate(0)",
                          "height": "350px"
                        }
                      },
                      ".icon-wrapper": {
                        "position": "absolute",
                        "width": "100px",
                        "height": "100px",
                        "display": "inline-flex",
                        "align-items": "center",
                        "justify-content": "center",
                        "border-radius": "100%",
                        "opacity": "0",
                        "z-index": "2",
                        "transition": "all 0.3s ease-in-out",
                        "overflow": "hidden",
                        "@media (max-width: 992px)": {
                          "display": "none"
                        },
                        ".mbr-iconfont": {
                          "font-size": "35px",
                          "color": "@icon",
                          "z-index": "1",
                          "transform": "rotate(45deg)"
                        },
                        "&:before": {
                          "content": "''",
                          "position": "absolute",
                          "width": "100px",
                          "height": "100px",
                          "background-color": "@iconBg",
                          "opacity": "0",
                          "border-radius": "100%",
                          "transition": "all 0.3s ease-in-out"
                        }
                      }
                    }
                  }
                },
                ".item-content": {
                  "position": "relative",
                  "z-index": "1",
                  "width": "250px",
                  "@media (max-width: 992px)": {
                    "margin-top": "45px",
