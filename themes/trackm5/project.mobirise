          "_name": "footer01",
          "_sourceTheme": "trackm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer0d1 trackm5\" group=\"Footer\" global once=\"footers\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Content\" name=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" condition=\"showMain\">\n        <input type=\"checkbox\" title=\"List 1\" name=\"showList_1\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"List 2\" name=\"showList_2\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" condition=\"showMain\" checked>\n        <input type=\"range\" inline title=\"Social Amount\" name=\"soc\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showMain\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#222222\" name=\"icon\" condition=\"showMain\" selected>\n        <input type=\"color\" title=\"Icon BG\" value=\"#ffffff\" name=\"iconBg\" condition=\"showMain\" selected>\n        <input type=\"color\" title=\"Icon Border\" value=\"#bdbdbd\" name=\"border\" condition=\"showMain\" selected>\n        <input type=\"color\" title=\"Active BG\" value=\"#3283fb\" name=\"active\" condition=\"showMain\" selected>\n        <input type=\"checkbox\" title=\"Contact 1\" name=\"showContact_1\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Contact 2\" name=\"showContact_2\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Contact Description\" name=\"showDescCont\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" condition=\"showMain\" checked>\n        <input type=\"color\" title=\"Container BG\" value=\"#ffffff\" name=\"container\" condition=\"showMain\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#e2e7f8\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fu