          "_name": "slider02",
          "_sourceTheme": "trackm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider02 mbr-embla trackm5\" group=\"Sliders &amp; Galleries\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked=\"\">\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" checked=\"\">\n        <input type=\"color\" title=\"Description BG\" value=\"#ffffff\" name=\"desc\" condition=\"showDesc\" selected=\"\">\n        <input type=\"color\" title=\"Container BG\" value=\"#3283fb\" name=\"container\" selected=\"\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked=\"\">\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\" checked=\"\">\n        <input type=\"color\" title=\"Arrows Color\" value=\"#3283fb\" name=\"arrowsColor\" condition=\"arrows\" selected=\"\">\n        <input type=\"color\" title=\"Arrows BG 1\" value=\"#ffffff\" name=\"arrow_1\" condition=\"arrows\" selected=\"\">\n        <input type=\"color\" title=\"Arrows BG 2\" value=\"#ffffff\" name=\"arrow_2\" condition=\"arrows\" selected=\"\">\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked=\"\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" name=\"showCardTitle\" title=\"Card Title\" checked=\"\">\n        <input type=\"color\" title=\"Card BG\" value=\"#ffffff\" name=\"card\" selected=\"\">\n        <input type=\"checkbox\" name=\"showIcon\" title=\"Card Icon\" checked=\"\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#ffffff\" name=\"icon\" condition=\"showIcon\" selected=\"\">\n        <input type=\"color\" title=\"Icon BG\" value=\"#3283fb\" name=\"iconBg\" condition=\"showIcon\" selected=\"\">\n        <header>Items</header>\n        <input type=\"checkbox\" name=\"showItems\" title=\"Items\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Amount\" name=\"items\" min=\"1\" max=\"3\" step=\"1\" value=\"3\" condition=\"showItems\">\n        <input type=\"checkbox\" name=\"showItemTitle\" title=\"Item Title\" condition=\"showItems\" checked=\"\">\n        <input type=\"checkbox\" name=\"showNumber\" title=\"Item Number\" condition=\"showItems\" checked=\"\">\n        <input type=\"color\" title=\"Item BG\" value=\"#e1edff\" name=\"item\" condition=\"showItems\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\" required=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#e2e7f8\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showDesc\">\n                <div class=\"desc-wrapper\">\n                    <p class=\"mbr-desc mbr-fonts-style\" data-app-selector=\".mbr-desc, .desc-wrapper\" mbr-theme-style=\"display-4\">\n                        Different genres\n                    </p>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-9 card\" mbr-if=\"showTitle\">\n                <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\">\n                    Producer Does Any <span>Kind of Music</span>\n                </h2>\n            </div>\n            <div class=\"col-12 card\">\n                <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                    <div class=\"embla__viewport\">\n                        <div class=\"embla__container\">\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-img\">\n                                            <img src=\"@PROJECT_PATH@/assets/images/image7.jpg\" alt=\"Mobirise Website Builder\">\n                                        </div>\n                                    </div>\n                                    <div class=\"item-content\">\n                                        <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" mbr-if=\"showCardTitle\">\n                                            Pop music\n                                        </h4>\n                                        <div class=\"items-wrapper\" mbr-if=\"showItems || showIcon\">\n                                            <div class=\"items-wrap\" mbr-if=\"showItems\">\n                                                <div class=\"item-wrap\">\n                                                    <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\">\n                                                        \"Dance All Night\"\n                                                    </h5>\n                                                    <p class=\"item-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-number\" mbr-if=\"showNumber\">\n                                                        4:00\n                                                    </p>\n                                                </div>\n                                                <div class=\"item-wrap\" mbr-if=\"items > 1\">\n                                                    <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\">\n                                                        \"Summer Love\"\n                                                    </h5>\n                                                    <p class=\"item-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-number\" mbr-if=\"showNumber\">\n                                                        4:00\n                                                    </p>\n                                                </div>\n                                                <div class=\"item-wrap\" mbr-if=\"items > 2\">\n                                                    <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\">\n                                                        \"Electric Dreams\"\n                                                    </h5>\n                                                    <p class=\"item-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-number\" mbr-if=\"showNumber\">\n                                                        4:00\n                                                    </p>\n                                                </div>\n                                            </div>\n                                            <a href=\"#\">\n                                                    <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-play mobi-mbri\" mbr-if=\"showIcon\"></span>\n                                            </a>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-img\">\n                                            <img src=\"@PROJECT_PATH@/assets/images/image5.jpg\" alt=\"Mobirise Website Builder\">\n                                        </div>\n                                    </div>\n                                    <div class=\"item-content\">\n                                        <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" mbr-if=\"showCardTitle\">\n                                            Hip-hop\n                                        </h4>\n                                        <div class=\"items-wrapper\" mbr-if=\"showItems || showIcon\">\n                                            <div class=\"items-wrap\" mbr-if=\"showItems\">\n                                                <div class=\"item-wrap\">\n                                                    <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\">\n                                                        \"City Streets\"\n                                                    </h5>\n                                                    <p class=\"item-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-number\" mbr-if=\"showNumber\">\n                                                        3:00\n                                                    </p>\n                                                </div>\n                                                <div class=\"item-wrap\" mbr-if=\"items > 1\">\n                                                    <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\">\n                                                        \"Rise to the Top\"\n                                                    </h5>\n                                                    <p class=\"item-number mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-number\" mbr-if=\"showNumber\">\n                                                        3:00\n                                                    </p>\n                                                </div>\n                                                <div class=\"item-wrap\" mbr-if=\"items > 2\