on=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"item features-without-image col-12 col-lg-4 item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <p class=\"card-number mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showNumber\" data-app-selector=\".card-number\">\n                            1k\n                        </p>\n                        <div class=\"title-wrap\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .title-wrap\">\n                                CASES THAT HAVE UNDERGONE LEGAL PROCEEDINGS\n                            </h4>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-lg-4 item-mb\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box\">\n                        <p class=\"card-number mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showNumber\" data-app-selector=\".card-number\">\n                            600\n                        </p>\n                        <div class=\"title-wrap\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .title-wrap\">\n                                CRIMINAL CASES COMPLETED AT THE INVESTIGATION STAGE\n                            </h4>\n                       