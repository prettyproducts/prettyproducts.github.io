lue=\"#d9d9d9\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#d9d9d9\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusPlaceholder\" title=\"Placeholders Focus\" value=\"#d9d9d9\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#6f5ad3\" selected=\"\">\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"card col-12 col-md-6 col-lg-4 mb-4\">\n                <div class=\"card-wrapper\">\n                    <h3 class=\"card-text align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-text\"><b><a href=\"#\" class=\"text-secondary\"><b>Brand building\n                                    ideas</b></a>\n                        </b>\n                        <div><b>Created daily</b></div>\n                    </h3>\n                    <div class=\"link-wrapper mt-4\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-down mobi-mbri\" mbr-if=\"showIcon\"></span>\n                        <div class=\"link-text-box\">\n                            <p class=\"link-text pt-2 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-text\"><b>Lorem ipsum dolor sit amet, consectetur&nbsp;\n                                    adipiscing\n                                    mauris scelerisque.</b></p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3\">\n                <div class=\"item\" mbr-if=\"itemsCount > 0\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Home</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 1\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">About</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 2\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Help</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 3\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Info</h3>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3\">\n                <div class=\"item\" mbr-if=\"itemsCount2 > 0\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Services</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount2 > 1\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Clients</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount2 > 2\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Features</h3>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount2 > 3\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">Contacts</h3>\n                </div>\n            </div>\n            <div class=\"col-lg-4 col-md-12\">\n                <h3 class=\"card-text align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-text\">Join Our Newsletter</h3>\n\n                <div mbr-form=\"\" class=\"mx-auto mbr-form\">\n                    <!--Formbuilder Form-->\n                    <form action=\"\" name=\"Form Name\" method=\"POST\" data-rcpha_sitekey=\"\" data-rcpha_secretkey=\"\" class=\"mbr-form form-with-styler\">\n                        <div class=\"form-row\">\n                            <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for\n                                filling out the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">\n                                Oops...! some problem!</div>\n                        </div>\n                        <div class=\"dragArea form-row\">\n                            <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"email\" style=\"\">\n                                <input type=\"email\" name=\"email\" placeholder=\"email@yoursite.com\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\">\n 