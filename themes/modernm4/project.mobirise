          "_customHTML": "<section class=\"header6 carousel slide testimonials-slider\" group=\"Headers\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider\" data-interval=\"false\">\n    \n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" title=\"Height Content\" name=\"heightContent\" min=\"500\" max=\"800\" step=\"20\" value=\"700\" condition=\"!fullScreen\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Icon Link\" name=\"showLink\" checked>\n        <input type=\"checkbox\" title=\"Show Mode\" name=\"showMode\" checked>\n        <input type=\"color\" title=\"Mode Color\" name=\"modeColor\" value=\"#c5ab50\" condition=\"showMode\">\n        <input type=\"checkbox\" title=\"Show Widget\" name=\"showWidget\" checked>\n         <input type=\"checkbox\" title=\"Show Icon Widget\" name=\"showIconWidget\" condition=\"showWidget\">\n        <input type=\"color\" title=\"Widget Color\" name=\"widgetColor\" value=\"#c5ab50\" condition=\"showWidget\">\n        <header>Slider</header>\n        <input type=\"range\" inline title=\"Count Items\" name=\"testimonialsSlides\" min=\"2\" max=\"5\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\">\n        <input type=\"range\" inline title=\"Autoplay Interval (ms)\" name=\"interval\" min=\"1000\" max=\"8000\" step=\"1000\" value=\"5000\" condition=\"showAutoplay\">\n        <input type=\"checkbox\" title=\"Show Controls\" name=\"showControls\" checked>\n        <input type=\"color\" title=\"Controls Color\" name=\"controlColor\" value=\"#000000\" condition=\"showControls\">\n        <input type=\"color\" title=\"Controls Color\" name=\"controBglColor\" value=\"#c5ab50\" condition=\"showControls\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class>\n        \n        <div class=\"carousel slide\" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval : 'false'}}\">\n            <div mbr-list mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner\">\n                <div class=\"carousel-item\">\n                    <div class=\"user\">\n                        <div class=\"mode\" mbr-if=\"showMode\"></div>\n                        <div class=\"title__block align-left\" mbr-if=\"showTitle || showSubtitle\">\n                            <h3 class=\"mbr-section-subtitle mbr-regular mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\">SPA SALON SERVICES</h3>\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .arrow__btn\">An oasis of \n<div>calm and beauty!</div></h1>\n                            <div class=\"arrow__btn\" mbr-if=\"showLink\">\n                            <a href=\"index.html\"><span mbr-icon class=\"mbr-iconfont mobi-mbri-right mobi-mbri\"></span></a>\n                        </div>\n                    </div>\n                            \n                        <div class=\"user_image\">\n                            <img src=\"@PROJECT_PATH@/assets/images/hans-vivek-uimkbvdqsaa-unsplash-1920x1280.jpg\" alt title>\n                        </div>\n                    </div>\n                </div>\n                \n            <div class=\"carousel-item\">\n                    <div class=\"user\">\n                        <div class=\"mode\" mbr-if=\"showMode\"></div>\n                        <div class=\"title__block align-left\" mbr-if=\"showTit