erlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n  </mbr-parameters>\n    \n    \n  <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n  </div>\n    \n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <div class=\"circle1\"></div>\n        <div class=\"circle2\"></div>\n        <div class=\"circle3\"></div>\n        <div class=\"circle4\"></div>\n        <div class=\"circle5\"></div>\n        <img src=\"@THEME_PATH@/components/_images/figure/triangle_green_transp.svg\" class=\"triangle\" alt>\n    </div>\n    \n    <div class=\"image__background\" mbr-if=\"showCircleEffect\">\n        <div class=\"circle__image1\"></div>\n        <div class=\"circle__image2\"></div>\n    </div>\n    \n  <div class=\"container container-table\">\n    <div class=\"title__block align-center col-lg-12\">\n        <h4 class=\"mbr-section-subtitle mbr-medium mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\">PRICING TABLES</h4>\n       <h3 class=\"mbr-section-title mbr-medium mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">Service Table</h3>\n    </div>\n\n      <div class=\"container scroll\">\n        <table class=\"table\" cellspacing=\"0\">\n          <thead>\n            <tr class=\"table-heads align-center\" mbr-list mbr-list-grow=\"tableColumns\">\n              <th class=\"head-item\"></th>\n\n\n\n              <th class=\"head-item\">\n                  <h5 class=\"table__title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".table__title\">&nbsp;</h5>\n                  <h4 class=\"table__price mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".table__price\">30 mins</h4>\n                  <h5 class=\"table__subtitle mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".table__subtitle\">&nbsp;</h5>\n              </th>\n              <th class=\"head-item\" mbr-theme-style=\"display-7\" data-app-selector=\".head-item\">\n                  <h5 class=\"table__title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".table__title\">&nbsp;</h5>\n                  <h4 class=\"table__price mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".table__price\">60 mins</h4>\n                  <h5 class=\"table__subtitle mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".table__subtitle\">&nbsp;</h5>\n              </th>\n            \n          </tr></thead>\n\n          <tbody mbr-list mbr-list-grow=\"tableRows\">\n            <tr mbr-list mbr-list-grow=\"tableColumns\">\n              <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".body-item1\">Facial treatments</td>\n\n\n\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$10</td>\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$20\n                </td>\n              \n            </tr>\n\n\n\n            <tr mbr-list mbr-list-grow=\"tableColumns\">\n              <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".body-item1\">Massages</td>\n\n\n\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$11</td>\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$22</td>\n              \n            </tr>\n            <tr mbr-list mbr-list-grow=\"tableColumns\">\n              <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".body-item1\">Body peels</td>\n\n\n\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$12</td>\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$24</td>\n              \n            </tr>\n            <tr mbr-list mbr-list-grow=\"tableColumns\">\n              <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".body-item1\">Wraps</td>\n\n\n\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$13</td>\n              <td mbr-text class=\"body-item mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item\">$26</td>\n              \n            </tr>\n            \n          </tbody>\n        </table>\n      </div>\n    </div>\n  \n</section>",
