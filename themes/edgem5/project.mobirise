\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#002549\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"align-center\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-text col-lg-{{textWidth}}\">\n                <p class=\"mbr-text label-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-4\" mbr-if=\"showLabel\" data-app-selector=\".label-text\"><b>GET IN TOUCH</b></p>\n                <h1 class=\"mbr-section-title mbr-fonts-style mb-0\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Get Your Right Solution, Contact <br>Now With Us.</b></h1>\n                <div mbr-buttons mbr-theme-style=\"display-5\" class=\"mbr-section-btn mt-3\" mbr-if=\"showButtons\"><a class=\"btn btn-white\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Contact Us</a></div>\n                <div class=\"advantages-container\" mbr-if=\"cardsAmount > 0\">\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 0\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            20+ years experience\n                        </p>\n                    </div>\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 1\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            Personal service\n                        </p>\n                    </div>\n                    <div class=\"advantage\" mbr-if=\"cardsAmount > 2\">\n                        <div class=\"advantage-icon\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-success mobi-mbri\" style=\"font-size: 10px;\"></span>\n                        </div>\n                        <p class=\"mbr-text advantage-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".advantage-text\">\n                            Chartered Tax Adviser\n                        </p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
