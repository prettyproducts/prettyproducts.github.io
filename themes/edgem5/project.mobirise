ition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"mbr-section-head\" mbr-if=\"showTitle || showSubtitle\">\n                <div class=\"section-title-container\" mbr-if=\"showTitle\">\n                    <h4 class=\"mbr-section-title mbr-fonts-style mb-0\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\"><b>Our Portfolio</b></h4>\n                </div>\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-0 mt-2\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipiscing elit</h5>\n            </div>\n        </div>\n    </div>\n    <div class=\"embla__viewport\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"embla__container\">\n            <div class=\"embla__slide slider-image item col-12 col-md-6 col-lg-4\">\n                <div class=\"sl-content\">\n                    <div class=\"item-wrap\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/features5.jpg\" alt title>\n                            <div class=\"img-filter\"></div>\n                        </div>\n                    </div>\n                    <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle || showText\">\n                        <div class=\"item-title-container\" mbr-if=\"showCardTitle\">\n                            <p class=\"item-title mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\"><span>DESIGN</span></p>\n                        </div>\n                        <p class=\"item-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-4\" data-app-selector=\"