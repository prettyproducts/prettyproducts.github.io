          "_name": "contacts1",
          "_sourceTheme": "powerm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"powerm5 contacts1\" group=\"Contact\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showMainSubtitle\" checked=\"\">\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcon\" checked=\"\">\n        <input type=\"checkbox\" name=\"showlinks\" title=\"Links\">\n        <input type=\"color\" title=\"Icon/Hover Color\" name=\"iconColor\" value=\"#232323\" selected=\"\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Card BG\" name=\"cardbg\" value=\"#ffffff\" selected=\"\" condition=\"showIcon\">\n        <select title=\"Columns\" name=\"columns\">\n            <option value=\"12\">1</option>\n            <option value=\"6\">2</option>\n            <option value=\"4\">3</option>\n            <option value=\"3\" selected=\"\">4</option>\n        </select>\n        <header>Background</header>\n        <input type=\"color\" name=\"bg1\" title=\"Color 1\" value=\"#ffbf31\">\n        <input type=\"color\" name=\"bg2\" title=\"Color 2\" value=\"#ffffff\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center mb-4\" mbr-if=\"showMainTitle || showMainSubtitle\">\n            <div class=\"col-12 mb-0 content-head\">\n                <div class=\"align w-100\">\n                    <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-white mb-3\" data-app-selector=\".mbr-section-subtitle, .align\" mbr-theme-style=\"display-7\" mbr-if=\"showMainSubtitle\">CONTACT\n                    </h2>\n                </div>\n\n                <h3 class=\"mbr-section-title mbr-fonts-style align-center\" mbr-theme-style=\"display-1\" mbr-if=\"showMainTitle\" data-app-selector=\".mbr-section-title\">If You Have Questions, <br>Please\n                    Contact Us\n                </h3>\n            </div>\n        </div>\n        <div class=\"row justify-content-center\">\n            <div class=\"item features-without-image col-12 col-md-6 col-lg-{{columns}}\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box align-left\">\n                        <div class=\"iconfont-wrapper mb-3\" mbr-if=\"showIcon\">\n                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-home mobi-mbri\"></span>\n                        </div>\n                        <h5 class=\"card-title mbr-fonts-style pb-4\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Main Office</h5>\n                        <p class=\"card-text mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">\n                            Oklahoma,&nbsp;Blanchard,<br>127 N. Main St</p>\n\n                        <div class=\"link-align w-100\" mbr-if=\"showlinks\">\n                            <div class=\"link-wrapper\">\n                                <h6 class=\"card-link mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-link, .link-icon, .link-align\">\n                                    Read More</h6>\n\n                            </div>\n                        </div>\n\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-md-6 col-lg-{{columns}}\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box align-left\">\n                        <div class=\"iconfont-wrapper mb-3\" mbr-if=\"showIcon\">\n                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-users mobi-mbri\"></span>\n                        </div>\n                        <h5 class=\"card-title mbr-fonts-style pb-4\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Support Chat</h5>\n                        <p class=\"card-text mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">support@yoursite.com<br>help@yoursite.com<br></p>\n\n                        <div class=\"link-align w-100\" mbr-if=\"showlinks\">\n                            <div class=\"link-wrapper\">\n                                <h6 class=\"card-link mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-link, .link-icon, .link-align\">\n                                    Read More\n                                </h6>\n\n                            </div>\n                        </div>\n\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-md-6 col-lg-{{columns}}\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box align-left\">\n                        <div class=\"iconfont-wrapper mb-3\" mbr-if=\"showIcon\">\n                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-map-pin mobi-mbri\"></span>\n                        </div>\n                        <h5 class=\"card-title mbr-fonts-style pb-4\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">\n                            Visit Us</h5>\n                        <p class=\"card-text mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">\n                            Bethlehem,&nbsp; Pennsylvania,<br>745 7th Ave<br></p>\n\n                        <div class=\"link-align w-100\" mbr-if=\"showlinks\">\n                            <div class=\"link-wrapper\">\n                                <h6 class=\"card-link mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-link, .link-icon, .link-align\">\n                                    <a href=\"#\" class=\"text-black\">Read More</a>\n                                </h6>\n\n                            </div>\n                        </div>\n\n                    </div>\n                </div>\n            </div>\n            <div class=\"item features-without-image col-12 col-md-6 col-lg-{{columns}}\">\n                <div class=\"item-wrapper\">\n                    <div class=\"card-box align-left\">\n                        <div class=\"iconfont-wrapper mb-3\" mbr-if=\"showIcon\">\n                            <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-phone mobi-mbri\"></span>\n                        </div>\n                        <h5 class=\"card-title mbr-fonts-style pb-4\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">\n                            Call Us</h5>\n                        <p class=\"card-text mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">\n                            +1(123)567-890<br>+1(123)890-567<br></p>\n\n                        <div class=\"link-align w-100\" mbr-if=\"showlinks\">\n                            <div class=\"link-wrapper\">\n                                <h6 class=\"card-link mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card-link, .link-icon, .link-align\">\n                                    <a href=\"#\" class=\"text-black\">Read More</a>\n                                </h6>\n\n                            </div>\n                        </div>\n\n                    </div>\n                </div>\n            </div>\n\n\n        </div>\n    </div>\n</section>",
