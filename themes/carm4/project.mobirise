        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@reverseContent)": {
              ".row": {
                "flex-direction": "row-reverse"
              }
            },
            ".img-wrap": {
              "position": "relative",
              "img": {
                "width": "100%"
              },
              ".icon-label": {
                "padding": "19px",
                "left": "auto",
                "top": "0",
                "right": "0",
                "bottom": "auto",
                "background-color": "@iconLabelBgColor",
                "z-index": "100",
                "position": "absolute",
                ".mbr-iconfont": {
                  "color": "#ffffff",
                  "font-size": "27px",
                  "line-height": "1"
                },
                "@media (max-width: 768px)": {
                  "left": "0",
                  "top": "auto",
                  "right": "auto",
                  "bottom": "0"
                }
              }
            },
            ".mbr-section-title": {
              "color": "#ffffff",
              "& when (@showTitleMask)": {
                "span": {
                  "line-height": "1",
                  "color": "@maskTextColor",
                  "padding": "0 6px 1px 5px",
                  "transform": "skew(-14deg)",
                  "background": "@maskColor",
                  "& when (@gradientMask)": {
                    "background": "linear-gradient(@maskColor 0%, @maskColor2 69%, @maskColor3 100%)"
                  },
                  "display": "inline-block",
                  "font-weight": "500",
                  "letter-spacing": "0",
                  "position": "relative"
                }
              }
            },
            ".mbr-section-subtitle": {
              "color": "#ffffff",
              "&.display-7": {
                "letter-spacing": "0"
              }
            },
            ".mbr-section-text": {
              "color": "#ffffff"
            },
            "& when (@customStyle)": {
              ".form-control": {
                "box-shadow": "none",
                "padding": "0.7em 1.07em 0em 0.8em",
                "background-color": "@fieldColor",
                "border": "0px solid @borderColor",
                "border-bottom": "1px solid @borderColor",
                "color": "@formText",
                "line-height": "1.43",
                "min-height": "3.5em",
                "border-radius": ".25rem",
                "&:focus, &:hover": {
                  "border": "0px solid @borderHover",
                  "border-bottom": "1px solid @borderHover",
                  "background-color": "@fieldHover"
                },
                "&.textarea": {
                  "padding-top": "40px"
                }
              },
              ".form-group": {
                "margin-bottom": "1.5rem"
              },
              "input::-webkit-input-placeholder, textarea::-webkit-input-placeholder": {
                "color": "@formPlaceholder"
              },
              "input:-moz-placeholder, textarea:-moz-placeholder": {
                "color": "@formPlaceholder"
              },
              ".jq-selectbox li, .jq-selectbox li": {
                "background-color": "@fieldColor",
                "color": "contrast(@fieldColor, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox li:hover, .jq-selectbox li.selected": {
                "background-color": "@fieldHover",
                "color": "contrast(@fieldHover, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox:hover .jq-selectbox__trigger-arrow": {
                "border-top-color": "@fieldColor"
              },
              ".jq-selectbox .jq-selectbox__trigger-arrow": {
                "border-top-color": "@fieldHover"
              }
            },
            "& when not (@customStyle)": {
              ".form-group": {
                ".wrap": {
                  "span": {
                    "display": "none"
                  }
                }
              }
            },
            ".text-blocks": {
              ".mbr-text": {
                "color": "#ffffff"
              }
            },
            "a:not([href]):not([tabindex])": {
              "color": "#fff"
            },
            ".multi-horizontal": {
              "flex-grow": "1",
              "-webkit-flex-grow": "1",
              "max-width": "100%"
            },
            ".input-group-btn": {
              "display": "block",
              "text-align": "left",
              ".btn-sm": {
                "padding": "7px 39px"
              }
            },
            "textarea.form-control": {
              "resize": "none"
            },
            "@media (max-width: 767px)": {
              "h2": {
                "padding-top": "2rem"
              },
              ".input-group-btn": {
                "text-align": "center !important"
              }
            },
            "@media (min-width: 768px)": {
              ".container > .row > .col-md-6": {
                "&:first-child": {
