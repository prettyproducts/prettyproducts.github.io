            },
            ".card": {
              "background-color": "if(@isTransparent, transparent, @spoilerColor)",
              "& when (@hasShadow)": {
                "box-shadow": "0px 10px 20px 0px rgba(29, 29, 27, 0.2)"
              }
            },
            ".title-section .mbr-section-title": {
              "text-align": "center"
            },
            ".card-title": {
              "flex": "1 1 auto"
            },
            ".accordion-body": {
              "padding-top": "1rem",
