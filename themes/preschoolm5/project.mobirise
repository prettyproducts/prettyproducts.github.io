            },
            ".heading-acc[aria-expanded=\"true\"]": {
              "& .acc-button": {
                "transform": "rotate(-45deg)"
              }
            },
            ".heading-acc:hover .acc-button": {
              "background-color": "@btnHoverColor",
              "& .acc-button-line1, & .acc-button-line2": {
                "background-color": "white"
              }
            },
            ".acc-button-container": {
              "& .acc-button": {
                "width": "40px",
                "height": "40px",
                "border-radius": "50%",
                "position": "relative",
                "transition": "0.3s linear",
                "& .acc-button-line1, & .acc-button-line2": {
                  "transition": "0.3s linear",
                  "position": "absolute",
