          "_customHTML": "<section data-bs-version=\"5.1\" class=\"gallery1\" group=\"Sliders & Galleries\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked>\n        <input type=\"checkbox\" name=\"showSubtitle\" title=\"Subtitle\" checked>\n        <header>Cards</header>\n        <input type=\"checkbox\" name=\"showCardTitle\" title=\"Title\" checked>\n        <input type=\"checkbox\" name=\"showCardSubtitle\" title=\"Subtitle\" checked>\n        <select name=\"leftColumn\" title=\"Left Column\">\n            <option value=\"1\">Row 1</option>\n            <option value=\"2\">Row 2</option>\n            <option value=\"3\" selected>Row 3</option>\n            <option value=\"4\">Row 4</option>\n        </select>\n        <select name=\"rightColumn\" title=\"Right Column\">\n            <option value=\"1\">Row 1</option>\n            <option value=\"2\" selected>Row 2</option>\n            <option value=\"3\">Row 3</option>\n            <option value=\"4\">Row 4</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#121212\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffffs\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    \n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"mbr-section-head\" mbr-if=\"showTitle || showSubtitle\">\n            <h5 class=\"mbr-section-subtitle ls-1 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">SHOWCASE —\n</h5>\n            <h4 class=\"mbr-section-title ls-1 mbr-fonts-style mb-0\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Start your project with us\n</h4>\n        </div>\n        \n        <div class=\"box\">\n            <div class=\"gallery-wrap-left gallery-wrap\">\n                <div class=\"gallery_item\">\n                    <div class=\"item-wrapper\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/mbr-11.jpg\" alt>\n                        </div>\n                        <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle\">\n                            <h5 class=\"item-title ls-1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\">\n                                Design\n                            </h5>\n                            <h6 class=\"item-subtitle ls-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-subtitle\" mbr-if=\"showCardSubtitle\">\n                                ART CREATIVE\n                            </h6>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"gallery-wrap-50\" mbr-if=\"leftColumn > 1\">\n                    <div class=\"row\">\n                        <div class=\"col-12 col-sm-6\">\n                            <div class=\"gallery_item item_left\">\n                                <div class=\"item-wrapper\">\n                                    <div class=\"item-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/mbr-7.jpeg\" alt>\n                                    </div>\n                                    <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle\">\n                                        <h5 class=\"item-title ls-1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\">\n                                            Illustration\n                                        </h5>\n                                        <h6 class=\"item-subtitle ls-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-subtitle\" mbr-if=\"showCardSubtitle\">\n                                            PHOTO DESIGN\n                                        </h6>\n                                    </div>\n                                </div>\n                            </div>    \n                        </div>\n                        <div class=\"col-12 col-sm-6\">\n                            <div class=\"gallery_item item_right\">\n                                <div class=\"item-wrapper\">\n                                    <div class=\"item-img\">\n                                        <img src=\"@PROJECT_PATH@/assets/images/mbr-8.jpeg\" alt>\n                                    </div>\n                                    <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle\">\n                                        <h5 class=\"item-title ls-1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\">\n                     