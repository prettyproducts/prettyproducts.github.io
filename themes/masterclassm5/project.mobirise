          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1\" group=\"Footers\" data-bg-video=\"{{bg.type == bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Info\" name=\"showInfo\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\">\n        <input type=\"checkbox\" title=\"Brand Name\" name=\"showBrand\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background2.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#295664\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper text-white\">\n            <div class=\"border-bottom\" mbr-if=\"showContacts\">\n                <div class=\"row\">\n                    <div class=\"col-6\">\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                            <a href=\"tel:+12345678910\" class=\"text-white\">0 (800) 123 45 67</a>\n                        </p>\n                    </div>\n                    <div class=\"col-6 text-end\">\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                            <a href=\"mailto:info@site.com\" class=\"text-white\">info@site.com</a>\n                        </p>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"border-bottom mt-4\" mbr-if=\"showTitle || showText || showInfo\">\n                <div class=\"row\">\n                    <div class=\"col-6 col-md-4 col-lg-3\">\n                        <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                            <b>Education</b>\n                        </h5>\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article mbr-if=\"showText\">\n                            <li class=\"mbr-text item-wrap\">Corporate training</li>\n                            <li class=\"mbr-text item-wrap\">Free lessons</li>\n                            <li class=\"mbr-text item-wrap\">Mobirise Kit</li>\n                            <li class=\"mbr-text item-wrap\">Analytics</li>\n                        </ul>\n                    </div>\n                    <div class=\"col-6 col-md-4 col-lg-3\">\n                        <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                            <b>Projects</b>\n                        </h5>\n                        <ul class=\"list mbr-fonts-style m-0\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article mbr-if=\"showText\">\n                            <li class=\"mbr-text item-wrap\">Media Netology</li>\n                            <li class=\"mbr-text item-wrap\">Career</li>\n                            <li class=\"mbr-text item-wrap\">Gift Certificate</li>\n                            <li class=\"mbr-text item-wrap\">Glossary</li>\n                        </ul>\n                    </div>\n                    <div class=\"col-6 col-md-4 col-lg-3\">\n                        <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                            <b>It is interesting</b>\n                        </h5>\n                        <ul class=\"list mbr-fonts-style m-0\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article mbr-if=\"showText\">\n                            <li class=\"mbr-text item-wrap\">Online courses</li>\n                            <li class=\"mbr-text item-wrap\">Our approach</li>\n                            <li class=\"mbr-text item-wrap\">Marketing</li>\n                            <li class=\"mbr-text item-wrap\">Subscribe to the newsletter</li>\n                        </ul>\n                    </div>\n                    <div class=\"col-6 col-md-4 col-lg-3\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article mbr-if=\"showInfo\">\n                            <li class=\"mbr-text item-wrap\">IE: Stephany Hart</li>\n                            <li class=\"mbr-text item-wrap\">TIN: 245804103265</li>\n                            <li class=\"mbr-text item-wrap\">PSR